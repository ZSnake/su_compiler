   -- A small function.
   function Sumsqr(X, Y: Integer) return integer is
   begin
      return X*X + Y*Y, ;
   end sumsqqr; ;

   -- una variable 
   I: Integer;
begin
   I := Sumsqr(3, 14);
   Put(I);
   New_Line; --comentar despues de algo 

   Put(Sumsqr(I, 4));
   New_Line;
 end f1;


